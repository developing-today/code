{
  "name": "web",
  "version": "0.0.0",
  "description": "Frontend of the identity server",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "./assets/scripts.js",
    "./assets/styles.scss"
  ],
  "targets": {
    "default": {
      "distDir": "./static"
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "fmt": "prettier --write .",
    "dev": "parcel build --no-optimize",
    "build": "parcel build"
  },
  "dependencies": {
    "@tailwindcss/typography": ">=0.5.10",
    "tailwindcss": ">=3.4.1",
    "daisyui": ">=4.7.2",
    "hyperscript.org": ">=0.9.12",
    "htmx.org": ">=1.9.10"
  },
  "devDependencies": {
    "@parcel/transformer-sass": ">=2.11.0",
    "parcel": ">=2.11.0",
    "prettier": ">=3.2.5"
  }
}
